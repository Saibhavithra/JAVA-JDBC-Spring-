package com.returnship.training.java8;

public class VarArgsdemo {
	public static void main(String[] args) {
		show(100);
		show(12,45);
		show(23,56,45);
		
	}

	 static void show(int.. a) {
		// TODO Auto-generated method stub
		 System.out.println(a.length);
		 for(int n:a) {
			 System.out.println(n); 
		 }
		
	}

}
